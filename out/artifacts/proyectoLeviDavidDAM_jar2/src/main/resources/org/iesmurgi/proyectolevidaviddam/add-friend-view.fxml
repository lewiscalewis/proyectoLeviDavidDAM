<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesmurgi.proyectolevidaviddam.Controllers.AddFriendController">
   <children>
      <VBox alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" spacing="20.0" styleClass="container" stylesheets="@css/main.css">
         <children>
            <VBox maxWidth="1000.0" minHeight="500.0" minWidth="500.0" spacing="50.0">
               <children>
                  <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="50.0" styleClass="browser">
                     <children>
                        <TextField fx:id="textfieldBrowser" onKeyTyped="#filterByName" prefHeight="35.0" prefWidth="333.0" />
                        <Button mnemonicParsing="false" onAction="#search" prefHeight="32.0" prefWidth="78.0" styleClass="browser-icon" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <ScrollPane fx:id="scrollPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" vbarPolicy="ALWAYS">
                     <content>
                        <VBox fx:id="container" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="40.0">
                           <padding>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                           </padding>
                        </VBox>
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
